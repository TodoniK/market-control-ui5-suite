<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Table items="{/}">
        <columns>
            <Column >
                <Text text="Id"/>
            </Column>
            <Column>
                <Text text="UserId"/>
            </Column>
            <Column>
                <Text text="ProductId"/>
            </Column>
            <Column>
                <Text text="Quantity"/>
            </Column>
            <Column>
                <Text text="CreatedAt"/>
            </Column>
        </columns>
        <items>
            <ColumnListItem>
                <cells>
                    <Text text="{id}"/>
                    <Text text="{user_id}"/>
                    <Text text="{product_id}"/>
                    <Text text="{quantity}"/>
                    <Text text="{created_at}"/>
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</core:FragmentDefinition>